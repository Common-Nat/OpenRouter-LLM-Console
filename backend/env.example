OPENROUTER_API_KEY=YOUR_KEY_HERE
APP_TITLE=OpenRouter LLM Console
APP_ORIGINS=http://localhost:5173
DB_PATH=./console.db
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_HTTP_REFERER=http://localhost:5173
OPENROUTER_X_TITLE=Self-Hosted LLM Console
OPENROUTER_TIMEOUT=90

# Rate Limiting (optional - defaults shown)
# Enable/disable all rate limiting
# RATE_LIMIT_ENABLED=true

# Critical resource-intensive endpoints
# RATE_LIMIT_STREAM=20 per minute          # LLM streaming requests
# RATE_LIMIT_MODEL_SYNC=60 per hour        # Model catalog sync
# RATE_LIMIT_UPLOAD=50 per minute          # Document uploads

# Standard CRUD operations
# RATE_LIMIT_SESSIONS=60 per minute        # Session management
# RATE_LIMIT_MESSAGES=100 per minute       # Message operations
# RATE_LIMIT_PROFILES=60 per minute        # Profile management

# Read-only operations (more lenient)
# RATE_LIMIT_MODELS_LIST=120 per minute    # Model listing
# RATE_LIMIT_USAGE_LOGS=120 per minute     # Usage statistics
# RATE_LIMIT_HEALTH_CHECK=300 per minute   # Health checks